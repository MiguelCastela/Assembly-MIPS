.data
array: .word 7,16,12,30,5,21,40,59,7,24
.text  
la $a0, array 
la $a1, array
la $a2, array

loop:
lw $t0, 0($a0)
andi $t1, $t0, 1
sw $t1, 0($a1)
sw $t1, 0($a2)
addi $a0, $a0, 4
addi $a1, $a1, 4
addi $t3, $t3, 1
beq $t3, 10, end_loop
j loop


end_loop:
li $v0, 1 
lw $a0, 0($a2)
addi $a2, $a2, 4 
syscall
addi $t4, $t4, 1
beq $t4, 10 , end
j end_loop

end:
